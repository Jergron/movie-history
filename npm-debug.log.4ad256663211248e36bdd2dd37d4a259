3232 silly install resolved     description: 'An easy-to-use wildcard globbing library.',
3232 silly install resolved     version: '0.1.0',
3232 silly install resolved     homepage: 'https://github.com/cowboy/node-globule',
3232 silly install resolved     author: { name: '"Cowboy" Ben Alman', url: 'http://benalman.com/' },
